<% provide(:title, 'Sign up') %>
<h1>Create new user</h1>
<div class="row">
<div class="col-md-6 col-md-offset-3">
	<% if current_user.tk == "A1" %>
	<%= render "createA2" %>
	<% end %>

	<% if current_user.tk == "A2" %>
	<%= render "createA3" %>
	<% end %>

	<% if current_user.tk == "A3" %>
	<%= render "createB1" %>
	<% end %>

	<% if current_user.tk == "B1" %>
	<%= render "createB2" %>
	<% end %>
</div>
</div>